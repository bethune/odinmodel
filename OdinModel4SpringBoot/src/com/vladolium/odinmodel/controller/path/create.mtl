[comment encoding = UTF-8 /]
[module create('http://www.eclipse.org/uml2/5.0.0/UML')]


[template public create(aClass : Class)
	{
		entityClassNameVar 		: String = aClass.name.toUpperFirst();
		entityPropertyNameVar	: String = aClass.name.toLowerFirst();
	}
]
@PostMapping("")
public [entityClassNameVar/] createOne(@RequestBody [entityClassNameVar/] [entityPropertyNameVar/]) {
	return [entityPropertyNameVar/]Service.createUpdate([entityPropertyNameVar/]);
}
[/template]
