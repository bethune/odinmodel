	- is not null.
--------------------------------------------------
/]
		[if p.lower = 1]
@NotNull
		[/if]
[comment : 
--------------------------------------------------
	- is unique.
--------------------------------------------------
/]
		[if p.isUnique = true]
			@Column(unique=true)
		[/if]
[comment : 
--------------------------------------------------
	- field definition.
--------------------------------------------------
/]
		private [p.type.name/] [p.name/];
	[/if]
[/for]
[/template]
