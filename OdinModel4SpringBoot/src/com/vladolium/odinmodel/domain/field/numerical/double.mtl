--------------------------------------------------
/]
[for (p: Property | aClass.getAllAttributes()->removeAll(aClass.getInheritedMembers())) separator('\n')]
[comment : 
--------------------------------------------------
	- is type integer.
--------------------------------------------------
/]
	[if p.type.name = 'Double']
[comment : 
