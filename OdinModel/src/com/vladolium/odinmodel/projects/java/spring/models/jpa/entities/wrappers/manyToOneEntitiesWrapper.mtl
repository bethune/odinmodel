[module manyToOneEntitiesWrapper('http://www.eclipse.org/uml2/5.0.0/UML')]

[**
 * Wraps entities from many-to-one relationships.
 * 
 * Necessary for the Inverse Referential Integrity Constraint (IRIC) 
 * implementation on a many-to-one relationship between two entities.
 * 
 * OMPOC (OdinModel Point Of Customization)
 * 
 * User may further customize package path and/or name, as well as file name, and Java imports.
 *   
 * @param entity represents classes from the UML class diagram i.e. the domain model.
 * @param relationship represents current class relationships from the UML class diagram i.e. the domain model.
*/]
[template public manyToOneEntitiesWrapper(entity : Class, relationship : Property){
	packagePath 			: String = entity._package.name.toLowerCase();
	packageName		 		: String = entity._package.name.replaceAll('/', '.').toLowerCase();
	entityNameUpperFirst	: String = entity.name.toUpperFirst();}]
[file ('src/main/java/' + packagePath + '/wrappers/' + relationship.opposite.type.name.toUpperFirst() + entityNameUpperFirst + '.java', false, 'UTF-8')]
package [packageName/].wrappers;

import [packageName/].model.*;

public class [relationship.opposite.type.name.toUpperFirst()/][entityNameUpperFirst/] {

    private [relationship.opposite.type.name.toUpperFirst()/] [relationship.opposite.type.name.toLowerFirst()/];

    private [entityNameUpperFirst/] [relationship.type.name.toLowerFirst()/];

    public [relationship.opposite.type.name.toUpperFirst()/] get[relationship.opposite.type.name.toUpperFirst()/]() {
	return [relationship.opposite.type.name.toLowerFirst()/];
    }

    public void set[relationship.opposite.type.name.toUpperFirst()/]([relationship.opposite.type.name.toUpperFirst()/] [relationship.opposite.type.name.toLowerFirst()/]) {
	this.[relationship.opposite.type.name.toLowerFirst()/] = [relationship.opposite.type.name.toLowerFirst()/];
    }

    public [entityNameUpperFirst/] get[entityNameUpperFirst/]() {
	return [relationship.type.name.toLowerFirst()/];
    }

    public void set[entityNameUpperFirst/]([entityNameUpperFirst/] [relationship.type.name.toLowerFirst()/]) {
	this.[relationship.type.name.toLowerFirst()/] = [relationship.type.name.toLowerFirst()/];
    }
}
[/file]
[/template]